                        </code>
                    </Medication>
                </x:context>
                <x:expect label="product_code/@originalText">
                    <product>
                        <product_code code="OTH" codeSystem="2.16.840.1.113883.5.1008" codeSystemName="HL7 NullFlavor" displayName="overig" originalText="Ureum 10% in eucerine cum aqua 100gr"/>
                    </product>
                </x:expect>
            </x:scenario>
