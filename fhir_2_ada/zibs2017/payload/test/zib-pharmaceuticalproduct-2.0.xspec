<x:description xmlns:x="http://www.jenitennison.com/xslt/xspec" xmlns:f="http://hl7.org/fhir" stylesheet="../zib-pharmaceuticalproduct-2.0.xsl">
    
    <x:scenario label="zib-pharmaceuticalproduct-2.0">
