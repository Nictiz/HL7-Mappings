        <x:scenario label="template name id-to-Identifier">
            <x:scenario label="@value and @root">
                <x:call template="id-to-Identifier">
                    <x:param name="in">
                        <identificatienummer value="000000000" root="2.16.840.1.113883.2.4.6.3"/>
                    </x:param>
                </x:call>
                <x:expect label="'system' and 'value'">
                    <f:system value="http://fhir.nl/fhir/NamingSystem/bsn"/>
                    <f:value value="000000000"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="@nullFlavor='MSK' and @root">
                <x:call template="id-to-Identifier">
                    <x:param name="in">
                        <identificatienummer nullFlavor="MSK" root="2.16.840.1.113883.2.4.6.3"/>
                    </x:param>
                </x:call>
                <x:expect label="'system' and 'value' with data-absent-reason extension">
                    <f:system value="http://fhir.nl/fhir/NamingSystem/bsn"/>
                    <f:value>
                        <f:extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                            <f:valueCode value="masked"/>
                        </f:extension>
                    </f:value>
                </x:expect>
            </x:scenario>
            <x:scenario label="other @nullFlavor and @root">
