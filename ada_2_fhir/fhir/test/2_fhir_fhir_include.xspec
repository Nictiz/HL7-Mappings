        <x:scenario label="template name string-to-string">
            <x:scenario label="input @value with leading and trailing space">
                <x:call template="string-to-string">
                    <x:param name="in">
                        <test value=" string "/>
                    </x:param>
                </x:call>
                <x:expect label="@value without leading and trailing space" select="f:out/@*">
                    <f:out value="string"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="input @otherAttribute">
                <x:call template="string-to-string">
                    <x:param name="in">
                        <test otherAttribute=" string "/>
                    </x:param>
                    <x:param name="inAttributeName" select="'otherAttribute'"/>
                </x:call>
                <x:expect label="@value" select="f:out/@*">
                    <f:out value="string"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="input empty @value">
                <x:call template="string-to-string">
                    <x:param name="in">
                        <test value=""/>
                    </x:param>
                </x:call>
                <x:expect label="@value" select="f:out/@*">
                    <f:out value="&#160;"/>
                </x:expect>
            </x:scenario>
            <x:scenario label="input no @value and @nullFlavor">
                <x:call template="string-to-string">
                    <x:param name="in">
                        <test nullFlavor="OTH"/>
                    </x:param>
                </x:call>
                <x:expect label="nullFlavor extension">
                    <f:extension url="http://hl7.org/fhir/StructureDefinition/iso21090-nullFlavor">
                        <f:valueCode value="OTH"/>
                    </f:extension>
                </x:expect>
            </x:scenario>
        </x:scenario>
        
