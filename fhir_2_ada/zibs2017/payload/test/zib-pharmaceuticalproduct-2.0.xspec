                            <text value="Ureum 10% in eucerine cum aqua 100gr"/>
                        </code>
                        <ingredient>
                            <itemCodeableConcept>
                                <coding>
                                    <system value="urn:oid:2.16.840.1.113883.2.4.4.7"/>
                                    <code value="15219127"/>
